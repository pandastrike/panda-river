{
  "name": "fairmont-reactive",
  "version": "1.0.0-beta-24",
  "description": "Functional reactive programming in JavaScript and CoffeeScript.",
  "files": [
    "src/",
    "lib/",
    "README.md"
  ],
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "coffee --nodejs '--harmony --harmony_proxies' -o lib/ -c src/*.*coffee",
    "test": "coffee --nodejs '--harmony --harmony_proxies' test/index.coffee",
    "watch": "coffee --nodejs '--harmony --harmony_proxies' -o lib/ -cw src/*.*coffee",
    "version": "(node_modules/.bin/json -f package.json version > VERSION)",
    "tag": "(cat VERSION | xargs -I version git tag -am version version) && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dyoder/fairmont.git"
  },
  "keywords": [],
  "authors": [
    "Dan Yoder"
  ],
  "license": "ISC",
  "devDependencies": {
    "amen": "^1.0.0-alpha-03",
    "fairmont": "^1.0.0-beta-19",
    "harmony-reflect": "^1.4.0",
    "json": "^9.0.3"
  },
  "dependencies": {
    "fairmont-core": "^1.0.0-beta-19",
    "fairmont-helpers": "^1.0.0-beta-18",
    "fairmont-multimethods": "^1.0.0-beta-18",
    "when": "^3.7.3"
  },
  "engine": "node >= 0.12.x"
}
